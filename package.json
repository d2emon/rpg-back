{
  "name": "rpg-back",
  "version": "0.1.0",
  "description": "Backend for RPG",
  "private": true,
  "main": "./src/run.ts",
  "scripts": {
    "lint": "tslint -c tslint.json -p tsconfig.json --fix",
    "prebuild": "npm run lint",
    "build": "tsc",
    "prestart": "npm run build",
    "start": "node ./dist/run.js",
    "debug": "node --debug-brk ./dist/run.js",
    "serve": "nodemon --watch src -e ts,ejs --exec npm run start",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/d2emon/rpg-back.git"
  },
  "keywords": [
    "rpg"
  ],
  "author": "Dmitry Kutsenko",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/d2emon/rpg-back/issues"
  },
  "homepage": "https://github.com/d2emon/rpg-back#readme",
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.13",
    "@types/mongoose": "^5.11.97",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.31",
    "nodemon": "^2.0.16",
    "tslint": "^6.1.3",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "debug": "^4.3.4",
    "express": "^4.18.1",
    "mongoose": "^6.3.2",
    "morgan": "^1.10.0",
    "winston": "^3.7.2"
  }
}
